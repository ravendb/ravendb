{
  "version": "4.0.0-*",
  "description": "Raven.Client is the client library for accessing RavenDB",
  "authors": [
    "Hibernating Rhinos"
  ],
  "packOptions": {
    "summary": "This package includes the client API of RavenDB. RavenDB is a document database for the .NET platform, offering a flexible data model design to fit the needs of real world systems.",
    "licenseUrl": "http://www.ravendb.net/licensing",
    "repository": {
      "type": "git",
      "url": "https://github.com/ravendb/ravendb"
    },
    "requireLicenseAcceptance": true,
    "iconUrl": "http://static.ravendb.net/logo-for-nuget.png",
    "owners": [
      "Hibernating Rhinos"
    ],
    "tags": [
      "ravendb",
      "client",
      "database",
      "nosql",
      "doc db"
    ],
    "projectUrl": "https://ravendb.net",
    "files": {
      "mappings": {
        "lib/netstandard1.3/": {
          "includeFiles": [
            "../Sparrow/bin/Release/netstandard1.3/Sparrow.dll"
          ]
        }
      }
    }
  },
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "define": [
      "PORTABLE"
    ],
    "compile": {
      "include": [
        "**/*.cs",
        "../CommonAssemblyInfo.cs"
      ]
    }
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Sparrow": {
      "version": "4.0.0-*",
      "type": "build",
      "target": "project"
    },
    "DotNetCross.Memory.Unsafe": "0.2.3.4",
    "Newtonsoft.Json": "9.0.1",
    "System.Net.Security": "4.3.0",
    "System.Runtime.InteropServices.RuntimeInformation": "4.3.0",
    "System.ServiceModel.Primitives": "4.3.0",
    "Lambda2Js": "2.1.1"
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "Microsoft.AspNetCore.Http.Features": "1.1.0",
        "Microsoft.CSharp": "4.3.0",
        "Microsoft.Extensions.Primitives": "1.1.0",
        "Microsoft.Win32.Primitives": "4.3.0",
        "System.Collections": "4.3.0",
        "System.Collections.Concurrent": "4.3.0",
        "System.Collections.Specialized": "4.3.0",
        "System.Console": "4.3.0",
        "System.Diagnostics.Contracts": "4.3.0",
        "System.Diagnostics.Debug": "4.3.0",
        "System.Diagnostics.Process": "4.3.0",
        "System.Diagnostics.Tracing": "4.3.0",
        "System.IO.Compression": "4.3.0",
        "System.IO.FileSystem": "4.3.0",
        "System.Linq": "4.3.0",
        "System.Linq.Queryable": "4.3.0",
        "System.Net.Http": "4.3.1",
        "System.Net.NameResolution": "4.3.0",
        "System.Net.Requests": "4.3.0",
        "System.Net.Sockets": "4.3.0",
        "System.Net.WebSockets": "4.3.0",
        "System.Net.WebSockets.Client": "4.3.0",
        "System.Reflection.Extensions": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0",
        "System.Resources.ResourceManager": "4.3.0",
        "System.Runtime.Extensions": "4.3.0",
        "System.Runtime.InteropServices": "4.3.0",
        "System.Security.Cryptography.Cng": "4.3.0",
        "System.Text.Encoding.Extensions": "4.3.0",
        "System.Threading": "4.3.0",
        "System.Threading.Thread": "4.3.0",
        "System.Threading.Timer": "4.3.0"
      }
    }
  }
}
{
  "version": "4.0.0-*",
  "description": "Voron - Low level storage engine",
  "authors": [ "Hibernating Rhinos" ],

  "packOptions": {
    "tags": [ "storage", "acid", "voron", "ravendb", "nosql" ]
  },

  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "compile": {
      "include": [ "**/*.cs", "../CommonAssemblyInfo.cs" ]
    }
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc2-*",
        "NETStandard.Library": "1.5.0-rc2-*",
        "System.Diagnostics.Process": "4.1.0-rc2-*",
        "System.IO.FileSystem.DriveInfo": "4.0.0-rc2-*",
        "System.IO.MemoryMappedFiles": "4.0.0-rc2-*",
        "System.Threading.Overlapped": "4.0.1-rc2-*"
      },
      "imports": "dnxcore50"
    }
  },
  "dependencies": {
    "Sparrow": "4.0.0-*"
  }
}

<Infrastructure:View x:Class="Raven.Studio.Views.StealthPagingDocuments"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:Infrastructure="clr-namespace:Raven.Studio.Infrastructure"
                     xmlns:Documents="clr-namespace:Raven.Studio.Features.Documents" mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400"
                     Title="Documents"
					 Style="{StaticResource PageStyle}"
					 Infrastructure:ModelAttacher.AttachObservableModel="StealthPagingDocumentsModel">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <Documents:StealthPagingDocumentsView DataContext="{Binding Value}"/>
    </Grid>
</Infrastructure:View>

									<GradientStop Color="#FF57001C" Offset="0.535"/>
									<GradientStop Color="#FF300116" Offset="0.986"/>
									<GradientStop Color="#FF5F404E" Offset="1"/>
								</LinearGradientBrush>
							</Border.Background>
							

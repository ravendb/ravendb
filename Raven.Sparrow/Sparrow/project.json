{
  "version": "3.5.0",

  "buildOptions": {
    "allowUnsafe": true,
    "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs" ]
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },

  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "NET4_5" ]
      }
    },
    "netstandard1.6": {
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [ "DNXCORE50" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1"
      }
    }
  }
}